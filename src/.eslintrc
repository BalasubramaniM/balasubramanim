{
    "rules": {
        "no-console": "warn",
        "no-debugger": "error",
        "no-func-assign": "error",
        "camelcase": "error",
        "key-spacing": [
            "error",
            {
                "beforeColon": false,
                "afterColon": true
            }
        ],
        "comma-spacing": [
            "error",
            {
                "before": false,
                "after": true
            }
        ],
        "no-trailing-spaces": "error",
        "no-multiple-empty-lines": [
            "error",
            {
                "max": 1
            }
        ],
        "no-nested-ternary": "error",
        "no-dupe-keys": "error",
        "eol-last":"error",
        "object-curly-spacing": ["error", "always"],
        "indent": ["error", 2]
    },
    "parser": "babel-eslint"
}